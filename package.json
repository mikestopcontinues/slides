{
  "name": "@sitearcade/hub-cms",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "start": "npm run dev",
    "dev": "parcel serve --cache-dir '.cache/parcel' --open -p 3070 './app/**/*.html'",
    "build": "parcel build --cache-dir '.cache/parcel' './app/**/*.html'",
    "deploy": "npm run clean && npm run build",
    "clean": "rm -rf .out .cache .tmp app/preval"
  },
  "dependencies": {
    "@sitearcade/dotenv": "1.1.3",
    "@sitearcade/is-env": "0.3.1",
    "@sitearcade/request": "1.2.4",
    "@sitearcade/uid": "0.3.1",
    "change-case": "4.1.2",
    "date-fns": "2.22.1",
    "fs-extra": "10.0.0",
    "ramda": "0.27.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "serve-static": "1.14.1",
    "spectacle": "8.3.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-typescript": "7.14.6",
    "@babel/preset-react": "7.14.5",
    "@sitearcade/browserslist-config": "0.3.1",
    "@sitearcade/eslint-config": "1.7.1",
    "@sitearcade/jest-preset": "4.2.1",
    "@sitearcade/prettier-config": "0.0.1",
    "@sitearcade/stylelint-config": "1.0.7",
    "@snowpack/plugin-typescript": "1.2.1",
    "@types/fs-extra": "9.0.11",
    "@types/ramda": "0.27.41",
    "@vercel/node": "1.11.1",
    "parcel": "2.0.0-beta.3.1",
    "ts-node": "10.0.0",
    "tsconfig-paths": "3.9.0",
    "typescript": "4.3.4",
    "vercel": "23.0.1"
  },
  "engines": {
    "node": "14.x"
  },
  "browserslist": [
    "extends @sitearcade/browserslist-config"
  ],
  "babel": {
    "presets": [
      "@parcel/babel-preset-env",
      "@babel/preset-react",
      "@babel/plugin-transform-typescript"
    ],
    "plugins": [
      "@parcel/babel-plugin-transform-runtime"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@sitearcade/eslint-config"
    ]
  },
  "stylelint": {
    "extends": [
      "@sitearcade/stylelint-config"
    ]
  },
  "jest": {
    "preset": "@sitearcade/jest-preset"
  },
  "prettier": "@sitearcade/prettier-config"
}
